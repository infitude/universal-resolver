@model ResolverViewModel

@{
    ViewBag.Title = "Resolve a DID";
}
@section scripts {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

    <div class="col-md-6 col-md-offset-3">
        <div class="well">
            <form method="post">
                <div asp-validation-summary="ModelOnly"></div>

                <div class="form-group">
                    <label asp-for="Did">DID:</label>
                    <input type="text" asp-for="Did" class="form-control"/>
                    <span asp-validation-for="Did" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <button type="submit" class="btn btn-primary"><i class="fa fa-id-badge"></i> Resolve DID</button>
                    <div>@ViewBag.UserMessage</div>
                </div>
            </form>
        </div>
    </div>